<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:local="using:Sensed.ViewModels"
             x:Class="Sensed.Views.ImagePreviewView"
             x:DataType="local:ImagePreviewViewModel" x:CompileBindings="True">

  <Design.DataContext>
    <local:ImagePreviewViewModel />
  </Design.DataContext>
  
  <Grid>
    <Image Source="{Binding Image}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
    <Button Content="Return" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="20"
            Command="{Binding ReturnCommand}"/>
  </Grid>
</UserControl>
